   pushd witch; ./test "$@"; git diff -U0 . | grep -E -v '^diff|^index|^Binary|^--|^\+-|\+\+|^@@|#Processed by|#DONE|in=|#ENDS' >> ../i.diff; popd
   eye --n3 i.diff --pass > /dev/null
}

pushd ~/github.com/josd/eye/i
i "$@" 2>&1 | tee i.txt
echo stamp,in,out,ent,step,brake,inf,sec,inf/sec > i.csv
cat i.txt | grep in= | sed -e 's/in=//' -e 's/out=//' -e 's/ent=//' -e 's/step=//' -e 's/brake=//' -e 's/inf=//' -e 's/sec=//' -e 's/inf\/sec=//' -e 's/ /,/g' >> i.csv
popd
