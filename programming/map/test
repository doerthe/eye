eye "$@" --n3 http://josd.github.io/eye/i/gps/gps-plugin.n3 --n3 http://josd.github.io/eye/i/map/agent2-map.n3 --proof http://josd.github.io/eye/i/map/agent1-proof.n3 --query http://josd.github.io/eye/i/map/agent2-query.n3 > agent2-proof.n3
